<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetDataGrid</servlet-name>
  	<display-name>GetDataGrid</display-name>
  	<description></description>
  	<servlet-class>dataGrid.GetDataGrid</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteGridData</servlet-name>
  	<display-name>DeleteGridData</display-name>
  	<description></description>
  	<servlet-class>dataGrid.DeleteGridData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddGridData</servlet-name>
  	<display-name>AddGridData</display-name>
  	<description></description>
  	<servlet-class>dataGrid.AddGridData</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetInfo</servlet-name>
  	<display-name>GetInfo</display-name>
  	<description></description>
  	<servlet-class>dataGrid.GetInfo</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetDataGrid</servlet-name>
  	<url-pattern>/GetDataGrid</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteGridData</servlet-name>
  	<url-pattern>/DeleteGridData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddGridData</servlet-name>
  	<url-pattern>/AddGridData</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetInfo</servlet-name>
  	<url-pattern>/GetInfo</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  	<welcome-file>grid.jsp</welcome-file>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
